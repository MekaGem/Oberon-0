MODULE Main;
	VAR n, ans : INTEGER;
	arr : ARRAY 100 OF INTEGER;

	PROCEDURE Fibonacci (n : INTEGER; VAR ans : INTEGER; arr : ARRAY 100 OF INTEGER);
		VAR ans1, ans2 : INTEGER;
	BEGIN
		IF (n = 1) OR (n = 2) THEN
			ans := 1
		ELSEIF (n = 2) THEN
			ans := 1
		ELSE
			Fibonacci(n - 1, ans1, arr);
			ans2 := arr[n - 2];
			ans := ans1 + ans2
		END;
		arr[n] := ans
	END Fibonacci;

BEGIN
	Read(n);
	Fibonacci(n, ans, arr);
	Write(ans);
	Write(arr[n]);
	WriteLn
END
